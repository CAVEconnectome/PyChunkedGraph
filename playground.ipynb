    "import numpy as np\n",
    "\n",
    "a = np.array([[1,2],[3,4]])\n",
    "b = np.concatenate([a, a[:, ::-1]], axis=0)\n",
    "print(a.view(dtype='u8'))\n",
    "print(b)\n",
    "\n",
    "v = b.view(dtype='u8,u8')\n",
    "print(v)\n",
    "\n",
    "n = b.shape[0]\n",
    "v = v.reshape(n)\n",
    "\n",
    "print(v)"
